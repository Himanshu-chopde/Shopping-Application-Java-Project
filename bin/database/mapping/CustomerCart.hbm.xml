<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="database.tables.CustomerCart" table="CustomerCart" dynamic-update="true">
		<composite-id>
			<key-property name="productId" column="productId"></key-property>
			<key-property name="customerEmail" column="customerEmail"></key-property>
		</composite-id>
		<property name="productQuantity" column="productQuantity"></property>
	</class>
</hibernate-mapping>