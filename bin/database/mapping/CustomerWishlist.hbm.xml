<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="database.tables.CustomerWishlist" table="CustomerWishlist" dynamic-update="true">
		<composite-id>
			<key-property name="productId"></key-property>
			<key-property name="customerEmail"></key-property>
		</composite-id>
	</class>
</hibernate-mapping>